{
    "name": "directee/directee",
    "type": "project",
    "description": "Directee JSON:API backend",
    "config": {
        "platform": {
            "php": "7.4"
        },
        "sort-packages": true
    },

    "autoload": {
        "psr-4": {
            "Directee\\": "src/"
        }
    },

    "minimum-stability": "stable",

    "repositories": [
      {
          "type": "vcs",
          "url": "https://github.com/directee/json-api-server"
      },
      {
        "type": "vcs",
        "url": "https://github.com/directee/interfaces"
    }
  ],

   "require": {
        "directee/interfaces": "@dev",
        "doctrine/dbal": "^3.4",
        "doctrine/lexer": "^1.2",
        "filp/whoops": "^2.15",
        "league/container": "^4.2",
        "monolog/monolog": "^2.9",
        "psr/container": "^2.0",
        "psr/event-dispatcher": "^1.0",
        "psr/http-client": "^1.0",
        "psr/log": "*",
        "tobyz/json-api-server": "dev-master",
        "wellrested/wellrested": "^5.1"
    }
}
